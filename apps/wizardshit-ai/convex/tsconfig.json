{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ES2021", "dom"],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"jsx": "react-jsx",
		"strict": true,
		"allowJs": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"types": ["node"]
	},
	"include": ["./**/*"],
	"exclude": ["./_generated"]
}
